{
  "name": "BNL Proxy",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Go to proxy.",
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["hostnames.json"],
      "matches": ["<all_urls>"],
      "extension_ids": []
    }
  ]
}